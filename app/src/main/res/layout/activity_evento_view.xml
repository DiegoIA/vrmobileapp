<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.studios.jedi.diegoa_hp.vr_explora_veracruz.Activities.EventoViewActivity">

    <ImageView
        android:id="@+id/iv_back5"
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:background="@android:color/darker_gray"
        tools:ignore="ContentDescription" />

    <ImageView
        android:id="@+id/iv_event"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/eventosbtn"
        tools:ignore="ContentDescription" />

    <TextView
        android:id="@+id/textCView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="19dp"
        android:text="@string/prueba"
        android:textColor="#eee"
        android:textSize="42sp"
        android:textStyle="bold"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/iv_event"
        android:layout_marginTop="20dp" />

    <com.github.snowdream.android.widget.SmartImageView
        android:id="@+id/smartiv"
        android:layout_width="150dp"
        android:layout_height="100dp"
        />

</RelativeLayout>
